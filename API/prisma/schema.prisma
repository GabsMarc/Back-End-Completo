generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model client {
  id       Int    @id @default(autoincrement())
  name     String @db.VarChar(255)
  lastname String @db.VarChar(255)
  age      Int
  email    String @unique
}

model product {
  id          Int    @id @default(autoincrement())
  name        String @db.VarChar(255)
  description String @db.VarChar(255)
}
